@using LectureSchedulingTool.Models

@{
    List<SVM.Teacher> teachers = ViewBag.teachers;
    if (teachers.Count != 0) { 
        for (int i = 0; i < teachers.Count; i++)
        {
            <option value="@teachers[i].id_teacher">
                @(teachers[i].ToString())
            </option>
        }
    }
    else
    {
        <option value="-1">
           ------
        </option>
    }
}
