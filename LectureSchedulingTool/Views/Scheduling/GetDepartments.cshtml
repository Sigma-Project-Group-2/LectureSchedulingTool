@using LectureSchedulingTool.Models

@{
    List<SVM.Department> departments = ViewBag.departments;

    if (departments.Count != 0)
    {
        for (int i = 0; i < departments.Count; i++)
        {
            <option value="@departments[i].id_department" >
                @(departments[i].ToString())
            </option>
        }
    }
    else
    {
        <option value="-1">
           ------
        </option>
    }
}